CMD:createhouse(playerid, params[])
{
	if(PlayerLogin[playerid] == 0) return SendClientMessage(playerid, -1, "[SYSTEM]: Anda Mestilah Login Untuk Menggunakan Cmd Ini");
    new cost, level, Float:x, Float:y, Float:z, string[MAX_PLAYERS], i = GetHouseModelID();
    if(PlayerInfo[playerid][pAdmin] >= 3)
    if(IsPlayerInAnyVehicle(playerid)) return SendClientMessage(playerid, 0xFF0000FF, "Sila Keluar Dari Kenderaan");
    if(sscanf(params, "dd", cost, level)) return SendClientMessage(playerid, -1, ""COL_RED"[INFO]"COL_WHITE" GUNAKAN: /createhouse [harga] [interior]");
    if(cost < 1) return SendClientMessage(playerid, -1, ""COL_RED"[INFO]"COL_WHITE" GUNAKAN: /createhouse [harga] [interior]");
    if(level < 0 || level > 8) return SendClientMessage(playerid, 0xFF0000FF, "Maaf, Interior Hanya [1-8]");
    GetPlayerPos(playerid, x, y, z);
    Rumah[i][Pos_X] = x, Rumah[i][Pos_Y] = y, Rumah[i][Pos_Z] = z;
    //
    Rumah[i][Price] = cost;
    Rumah[i][Int] = level;
    Rumah[i][Owned] = 0;
    Rumah[i][VirWorld] = i;
    format(Rumah[i][Owner], MAX_PLAYER_NAME, "%s", "Tiada");
    new INI:file = INI_Open(HouseFile(i));
    INI_WriteInt(file, "Owned", 0);
    INI_WriteString(file, "Owner", Rumah[i][Owner]);
    INI_WriteInt(file, "Price", cost);
    INI_WriteFloat(file, "hEntranceX", Rumah[i][Pos_X]);
    INI_WriteFloat(file, "hEntranceY", Rumah[i][Pos_Y]);
    INI_WriteFloat(file, "hEntranceZ", Rumah[i][Pos_Z]);
    INI_WriteInt(file, "Interior", Rumah[i][Int]);
    INI_WriteInt(file, "VirtualWorld", Rumah[i][VirWorld]);
    INI_Close(file);
    format(string, sizeof(string), "Pemilik: Tiada\nHarga: RM%d\nRumah: %d", Rumah[i][Price], i);
    Rumah[i][Label] = CreateDynamic3DTextLabel(string, 0xFFFFFFFF, Rumah[i][Pos_X], Rumah[i][Pos_Y], Rumah[i][Pos_Z], 50.0, INVALID_PLAYER_ID, INVALID_VEHICLE_ID, 1, -1, -1, -1, 10.0);
    format(string, sizeof(string), "Anda telah membuat rumah | Model: %d | Interior: %d | Harga: RM%d", i, Rumah[i][Int], Rumah[i][Price]);
    SendClientMessage(playerid, 0xFFFF00FF, string);
    return 1;
}

