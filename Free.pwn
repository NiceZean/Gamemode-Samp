stock SendMessageInChat(playerid, text[], Float: radius = 30.0)
{
	new Float: dist, type;
	new Float: x, Float: y, Float: z;

	GetPlayerPos(playerid, x, y, z);
	SetPlayerChatBubble(playerid, text, 0xFFFFFFFF, 30.0, 8000);

	ChatMessageInit(playerid, text);
	foreach(new idx : Player)
	{
    	if(GetPVarInt(playerid, "TryToEscape") == 1)
		dist = GetPlayerDistanceFromPoint(idx, x, y, z);

		if(dist > radius) continue;
		if(dist < (radius / 4))
		{
			SendClientMessage(idx, 0xFFFFFFFF, chat_message[type]);
		}
		else if(dist < (radius / 2))
		{
			SendClientMessage(idx, 0xFFFFFFFF, chat_message[type]);
		}
		else
		{
			SendClientMessage(idx, 0xFFFFFFFF, chat_message[type]);
		}
	}
	if(!IsPlayerInAnyVehicle(playerid))
	{
		ApplyAnimation(playerid, "PED", "IDLE_chat", 4.100, 0, 1, 1, 1, 1, 1);
		SetTimerEx("HentiAnim", strlen(text) * 400, false, "i", playerid);
	}
	return 1;
}

