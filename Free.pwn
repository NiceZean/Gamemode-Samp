      GameTextForPlayer(targetid, "~g~ANDA TELAH MENJADI AHLI YAKUZA", 5000, 1);
      format(string, sizeof string, ":trident: **___INFO KEMASUKKAN ROLE___**\n`Nama : %s\nRole : YAKUZA` :dragon:\n`Leader : %s` ", GetName(targetid), GetName(playerid));
	  DCC_SendChannelMessage(g_Discord_kemasukkanrole, string);
   }
   return 1;
}

CMD:jualdadah(playerid, params[])
