    		SendClientMessageToAll(COLOR_RED, string);
    		SetTimerEx("Delay_Kick", 100, false, "i", playerid);
    		format(string, sizeof string, ":gun: **__HACK WEAPON__**\n`Nama Pemain : %s\nNama Senjata : Jetpack`", GetName(playerid));
            DCC_SendChannelMessage(g_Discord_hackweapon, string);
    	}
    }
    new Float: playersHealth;
    GetPlayerHealth(playerid, playersHealth);
    if(playersHealth <= 10 && playersHealth != 0)
    {
    	PlayerInfo[playerid][pInjured] -= 1;
        SetPlayerDrunkLevel(playerid, 8000);
        SetPlayerHealth(playerid, 10);
        ApplyAnimation(playerid,"SWEET","Sweet_injuredloop", 4.0, 1, 0, 0, 0, 0);
        GameTextForPlayer(playerid, "~r~INJURED", 2000, 3);
        format(string, sizeof(string), ""COL_RED"[INJURED]"COL_WHITE" Anda Akan Mati Dalam Masa %d Saat", PlayerInfo[playerid][pInjured]);
        SendClientMessage(playerid, -1, string);
    }
    if(PlayerInfo[playerid][pCarry] == 1)
    {
        new Float:X, Float:Y, Float:Z;
        new targetid = PlayerInfo[playerid][pCarried];
        GetPlayerPos(playerid, X, Y, Z);
        new vw = GetPlayerVirtualWorld(playerid);
        new interior = GetPlayerInterior(playerid);
        SetPlayerPos(targetid, X+1, Y+1, Z);
        SetPlayerVirtualWorld(targetid, vw);
        SetPlayerInterior(targetid, interior);
        PlayerInfo[targetid][pCarry] = 1;
        PlayerInfo[targetid][pCarried] = playerid;
    }
    if(PlayerInfo[playerid][pInjured] <= 0)
    {
    	SetPlayerHealth(playerid, 0);
    	PlayerInfo[playerid][pInjured] = 60;
    }
    if(PlayerInfo[playerid][pWarn] == 3)
    {
    	format(string, sizeof(string), ""COL_DC"[BOT]"COL_RED"%s"COL_WHITE" Telah Ditendang Keluar Dari Server "COL_RED"(Kesalahan : Warning Kali Ketiga)", GetName(playerid));
    	SendClientMessageToAll(COLOR_RED, string);
    	SetTimerEx("Delay_Kick", 100, false, "i", playerid);
    	format(string, sizeof string, ":gun: **__INFO DISCORD KICK__**\n`Nama Pemain : %s\nKesalahan : Warning Kali Ketiga`", GetName(playerid));
        DCC_SendChannelMessage(g_Discord_hackweapon, string);
    }
    //
    //==========================================================================================
	return 1;
}

