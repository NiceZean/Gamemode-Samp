CMD:transfer(playerid, params[])
{
	if(PlayerLogin[playerid] == 0) return SendClientMessage(playerid, -1, "[SYSTEM]: Anda Mestilah Login Untuk Menggunakan Cmd Ini");
    new targetid, jumlah, string[MAX_PLAYERS];
    if(PlayerInfo[playerid][pBank] >= 1 )
    {
        if(sscanf(params, "ud", targetid, jumlah)) return SendClientMessage(playerid,-1,""COL_RED"[INFO]"COL_WHITE" GUNAKAN: /transfer [playerid][jumlah]");
        if(!IsPlayerConnected(targetid)) return SendClientMessage(playerid,-1,""COL_DC"[BOT]:"COL_WHITE" Player Ini Tidak Online");
        if(jumlah < 0 || jumlah > PlayerInfo[playerid][pBank]) return SendClientMessage(playerid,-1,""COL_LGREEN"[MAYBANK]"COL_WHITE" Anda Tidak Mempunyai Duit Sebanyak Itu Di Akaun Bank Anda");
        format(string, sizeof(string), ""COL_LGREEN"[MAYBANK]"COL_WHITE" Anda Telah Mentransferkan Duit "COL_RED"RM%d Kepada Akaun Bank %s", jumlah, GetName(targetid));
        SendClientMessage(playerid, -1, string);
        format(string, sizeof(string), ""COL_LGREEN"[MAYBANK]"COL_WHITE" %s Telah Mentransfer Duit Sebanyak "COL_LGREEN"RM%d "COL_WHITE"Kepada Akaun Bank Anda",GetName(playerid), jumlah);
        SendClientMessage(targetid, -1, string);
        PlayerInfo[playerid][pBank] -= jumlah;
        PlayerInfo[targetid][pBank] += jumlah;
    }
    else SendClientMessage(playerid, -1, ""COL_RED"[INFO]"COL_WHITE" Anda Tidak Mempunyai Wang Yang Cukup Di Akaun Bank Anda!");
    return 1;
}

