CMD:fac(playerid, params[])
{
	if(PlayerLogin[playerid] == 0) return SendClientMessage(playerid, -1, "[SYSTEM]: Anda Mestilah Login Untuk Menggunakan Cmd Ini");
	new string[128], text[128], ctext[60], pname[MAX_PLAYER_NAME+1];
	GetPlayerName(playerid, pname, sizeof(pname));
	if(sscanf(params, "s[128]", text)) return SendClientMessage(playerid, 0x46E850FF, ""COL_RED"[INFO]"COL_WHITE"GUNAKAN: /f(ac) [text]");
	if(PlayerInfo[playerid][pMSPD] >= 1  || PlayerInfo[playerid][pMSMD] >= 1 || PlayerInfo[playerid][pRTSD] >= 1)
	{
        if(PlayerInfo[playerid][pMSPD] >= 1){ ctext = "Anggota MSPD"; }
		if(PlayerInfo[playerid][pMSPD] == 5){ ctext = "Ketua Polis"; }
		if(PlayerInfo[playerid][pMSMD] >= 1){ ctext = "Ahli MSMD"; }
		if(PlayerInfo[playerid][pMSMD] == 5){ ctext = "Pengarah MSMD"; }
		if(PlayerInfo[playerid][pRTSD] >= 1){ ctext = "Anggota RTSD"; }
		if(PlayerInfo[playerid][pRTSD] == 5){ ctext = "Ketua RTSD"; }
		format(string, sizeof(string), "(( %s %s: %s ))", ctext, pname, text);
		if(PlayerInfo[playerid][pMSPD] > 0 || PlayerInfo[playerid][pMSMD] > 0 || PlayerInfo[playerid][pRTSD] > 0)
		{
			SendGoodsideMessage(COLOR_RED, string);
		}
	}
	return 1;
}

