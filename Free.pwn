public OnPlayerPickUpDynamicPickup(playerid, pickupid)
{
    loop(u, MAX_HOUSES, 1)
    {
        if(pickupid == Rumah[u][Pickup])
        {
            if(GetPVarInt(playerid, "WeAreHome") == 0)
            {
                if(Rumah[u][Owned] == 1)
                {
                    GameTextForPlayer(playerid, "~w~TEKAN BUTANG ~r~Y ~w~UNTUK MASUK RUMAH INI~n~ ~w~TEKAN BUTANG ~r~ALT ~w~UNTUK JUAL RUMAH INI", 3000, 3);
                }
                else
                {
                    GameTextForPlayer(playerid, "~w~TEKAN BUTANG ~r~ALT ~w~UNTUK BELI RUMAH INI", 3000, 3);
                }
            }
        }
    }
    if(pickupid == Exit_House[0] || pickupid == Exit_House[2] ||
