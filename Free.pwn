public OnPlayerText(playerid, text[])
{
	if(PlayerLogin[playerid] == 0) return SendClientMessage(playerid, -1, "[SYSTEM]: Login dulu bodo kalau nak chat");
	new string[256];
	GetPlayerName(playerid, sendername, sizeof(sendername));
	if(MaskRob[playerid] == 1) {
		format(string, sizeof(string), "{1cc7aa}[MASKED] {c99718}: {33ff00}%s", text);
	}
	else {
        format(string, sizeof(string), "{1cc7aa}[TALK] {c99718}%s: {33ff00}%s", sendername, text);
	}
	ProxDetector(20.0, playerid, string,COLOR_FADE1,COLOR_FADE2,COLOR_FADE3,COLOR_FADE4,COLOR_FADE5);
    return 0;
}

