public OnGameModeExit()
{
	new string[MAX_PLAYERS], year, month, day, hour, mins, sec;
	InCountDown = 0;
	//
	for(new i = 0; i < MAX_PLAYERS; i++)
	{
	    getdate(year, month, day);
		gettime(hour, mins, sec);
		format(string, sizeof(string), "%s%d/%s%d/%s%d @ %s%d:%s%d:%s%d", ((day < 10) ? ("0") : ("")), day, ((month < 10) ? ("0") : ("")), month, (year < 10) ? ("0") : (""), year, (hour < 10) ? ("0") : (""), hour, (mins < 10) ? ("0") : (""), mins, (sec < 10) ? ("0") : (""), sec);
		new INI:file = INI_Open(UserPath(i));
	    INI_SetTag(file, "data");
	    INI_WriteString(file, "Last_Seen", string);
		INI_Close(file);
		PlayerInfo[i][pSpawn] = 0;
	}
	return 1;
}

