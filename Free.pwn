public OnPlayerGiveDamage(playerid, damagedid, Float:amount, weaponid, bodypart)
{
	new Float:health, Float:armour;
	GetPlayerHealth(damagedid, health);
	GetPlayerArmour(damagedid, armour);
	if(armour == 0.0)
	{
	    health -= amount;
	    if(health < 0.0) health = 0;
	}
	else
	{
	    if(armour-amount < 0.0)
	    {
	        health -= (amount - armour);
	        armour = 0;
	    }
	    else armour -= amount;
	}
	SetPlayerHealth(damagedid, health);
	SetPlayerArmour(damagedid, armour);
	new Float:healthgod;
	GetPlayerHealth(damagedid, healthgod);
	if(healthgod > 99)
	if(GODMODEADMIN[damagedid] == 0)
	if(GODMODE[damagedid] == 0)
	{
	new string[128], victim[MAX_PLAYER_NAME];
	GetPlayerName(damagedid, victim, sizeof (victim));
	format(string, sizeof(string), "{e67e00}[MRRP BOT]: {ffffff}%s telah di tendang dari server kerana {ff0000}(GOD MODE)", victim);
 	SendClientMessageToAll(-1, string);
 	SetTimerEx("Tendang", 500, false, "i", damagedid);
 	}
	return 1;
}

